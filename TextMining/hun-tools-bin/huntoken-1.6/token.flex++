	/********************************************************************************/
	/* Mondatok felismerése, feldolgozó állapotba (S) lépés                         */

<INITIAL>^"<s>" {
	*yyout << YYText();
	strcpy(msd, OPEN);
	multitoken = NORMAL;
	abort_slashdot = 0;
	BEGIN(SENTENCE_BEGIN);
}

<SENTENCE_BEGIN>[^\n]*"\n" {
        /* mondat külön kiírása */
	*yyout <<  "%s", YYText();

	yyless(0);
	BEGIN(S);
}

